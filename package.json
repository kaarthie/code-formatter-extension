{
  "name": "code-formatter",
  "displayName": "code-formatter",
  "description": "Extension for organizing the code ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Venusubashhhh/code-formatter.git" 
    },
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeformatter.generateDocumentation",
        "title": "Generate Documentation"
      },
      {
        "command": "backend.createFolders",
        "title": "Generate Backend Structure"
      },
      {
        "command": "frontend.createFolders",
        "title": "Generate Frontend Structure"
      },
      {
        "command": "extension.removeCommentsAndLogs",
        "title": "Remove Comments and log statements"
      },
      {
        "command": "extension.removeEmptyFiles",
        "title": "Remove Empty Files and Folders"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.80.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "openai": "^4.26.0"
  },
  "icon": "https://cdn-icons-png.flaticon.com/512/1458/1458497.png"
}
